<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <!-- Equipement Type -->
        <record id="is_equipement_type_view_tree" model="ir.ui.view">
            <field name="name">view.is.equipement.type.tree</field>
            <field name="model">is.equipement.type</field>
            <field name="arch" type="xml">
                <tree string="Type equipement">
                    <field name="name" />
                    <field name="code" />
                    <field name="is_database_origine_id" />
                </tree>
            </field>
        </record>
        <record id="is_equipement_type_view_form" model="ir.ui.view">
            <field name="name">view.is.equipement.type.form</field>
            <field name="model">is.equipement.type</field>
            <field name="arch" type="xml">
                <form string="Type equipement">
                    <header>
                        <button 
                            name="add_champs_action"
                            string="Add champs"
                            type="object"
                            class="oe_highlight"
                        />
                    </header>
                    <sheet>
                        <group>
                            <group>
                                <field name="name" />
                            </group>
                            <group>
                                <field name="code" />
                            </group>
                        </group>
                        <field name="champ_line_ids" nolabel="1">
                            <tree editable="bottom">
                                <field name="name" required="1 "/>
                                <field name="vsb" />
                                <field name="obligatoire" />
                            </tree>
                        </field>
                        <group>
                        <field name="active" />
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="is_equipement_type_filter" model="ir.ui.view">
            <field name="name">is_equipement_type_filter</field>
            <field name="model">is.equipement.type</field>
            <field name="arch" type="xml">
                <search string="Type equipement">
                    <field name="name" />
                    <field name="code" />
                </search>
            </field>
        </record>
        <record id="action_is_equipement_type" model="ir.actions.act_window">
            <field name="name">Type equipement</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">is.equipement.type</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>

